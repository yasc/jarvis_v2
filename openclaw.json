{
  "agents": {
    "defaults": {
      "model": {
        "primary": "anthropic/claude-sonnet-4-5",
        "fallbacks": ["anthropic/claude-opus-4-6"]
      },
      "thinkingDefault": "low",
      "heartbeat": {
        "every": "2h",
        "target": "last",
        "activeHours": {
          "start": "07:00",
          "end": "22:00",
          "timezone": "Europe/London"
        }
      }
    },
    "list": [
      {
        "id": "main",
        "default": true,
        "identity": {
          "name": "Jarvis",
          "theme": "warm, practical household AI assistant",
          "emoji": "üè†"
        },
        "groupChat": {
          "mentionPatterns": ["jarvis", "Jarvis", "@jarvis", "hey jarvis", "yo jarvis", "J"]
        }
      }
    ]
  },
  "channels": {
    "telegram": {
      "enabled": true,
      "dmPolicy": "pairing",
      "allowFrom": ["68093606", "64004206"],
      "groups": {
        "-5259961230": {
          "enabled": true,
          "requireMention": false
        }
      }
    }
  },
  "session": {
    "dmScope": "per-channel-peer"
  },
  "tools": {
    "allow": ["read", "write", "exec", "web_search", "web_fetch"]
  },
  "skills": {
    "entries": {
      "gog": {
        "enabled": true,
        "env": {
          "GOG_KEYRING_PASSWORD": "$GOG_KEYRING_PASSWORD",
          "GOG_ACCOUNT": "$GOG_ACCOUNT"
        }
      },
      "icloud-calendar": {
        "enabled": true,
        "env": {
          "ICLOUD_EMAIL": "$ICLOUD_EMAIL",
          "ICLOUD_APP_PASSWORD": "$ICLOUD_APP_PASSWORD"
        }
      }
    }
  },
  "hooks": {
    "enabled": true,
    "token": "${OPENCLAW_GATEWAY_TOKEN}",
    "allowRequestSessionKey": true,
    "allowedSessionKeyPrefixes": ["hook:"]
  }
}
